{
  "dashboard": {
    "id": null,
    "uid": "mayo-strip-ai",
    "title": "Mayo Clinic STRIP AI - API Metrics",
    "tags": ["mayo", "ml", "api", "medical"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 0,
    "refresh": "5s",
    "panels": [
      {
        "id": 1,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "type": "graph",
        "title": "Request Rate (req/sec)",
        "targets": [
          {
            "expr": "rate(api_requests_total[1m])",
            "legendFormat": "{{endpoint}} - {{method}} ({{status}})",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "reqps", "label": "Requests/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 2,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "type": "graph",
        "title": "Request Latency (p50, p95, p99)",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(api_request_duration_seconds_bucket[5m]))",
            "legendFormat": "p50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(api_request_duration_seconds_bucket[5m]))",
            "legendFormat": "p95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(api_request_duration_seconds_bucket[5m]))",
            "legendFormat": "p99",
            "refId": "C"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Latency"},
          {"format": "short"}
        ]
      },
      {
        "id": 3,
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 8},
        "type": "stat",
        "title": "Active Requests",
        "targets": [
          {
            "expr": "api_active_requests",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "fields": "",
            "calcs": ["lastNotNull"]
          }
        },
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 50, "color": "yellow"},
                {"value": 100, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 8},
        "type": "stat",
        "title": "Model Status",
        "targets": [
          {
            "expr": "model_loaded",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "background",
          "graphMode": "none",
          "justifyMode": "auto",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "fields": "",
            "calcs": ["lastNotNull"]
          },
          "textMode": "value_and_name"
        },
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {"type": "value", "value": "0", "text": "Not Loaded"},
              {"type": "value", "value": "1", "text": "Loaded"}
            ],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 1, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 8},
        "type": "stat",
        "title": "Error Rate (last 5m)",
        "targets": [
          {
            "expr": "sum(rate(api_errors_total[5m]))",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "fields": "",
            "calcs": ["lastNotNull"]
          }
        },
        "fieldConfig": {
          "defaults": {
            "unit": "errors/sec",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 0.1, "color": "yellow"},
                {"value": 1, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 6,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
        "type": "graph",
        "title": "Predictions by Class",
        "targets": [
          {
            "expr": "rate(predictions_total[5m])",
            "legendFormat": "{{predicted_class}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "ops", "label": "Predictions/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 7,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
        "type": "graph",
        "title": "Prediction Confidence Distribution",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(prediction_confidence_bucket[5m]))",
            "legendFormat": "p50 - {{predicted_class}}",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(prediction_confidence_bucket[5m]))",
            "legendFormat": "p95 - {{predicted_class}}",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "percentunit", "label": "Confidence", "min": 0, "max": 1},
          {"format": "short"}
        ]
      },
      {
        "id": 8,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24},
        "type": "graph",
        "title": "Batch Prediction Sizes",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(batch_prediction_size_bucket[5m]))",
            "legendFormat": "p50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(batch_prediction_size_bucket[5m]))",
            "legendFormat": "p95",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Batch Size"},
          {"format": "short"}
        ]
      },
      {
        "id": 9,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24},
        "type": "graph",
        "title": "Error Breakdown by Type",
        "targets": [
          {
            "expr": "rate(api_errors_total[5m])",
            "legendFormat": "{{endpoint}} - {{error_type}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "errors/sec", "label": "Errors/sec"},
          {"format": "short"}
        ]
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h"]
    },
    "templating": {
      "list": []
    }
  }
}
